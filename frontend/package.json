    "@tanstack/react-query": "^5.90.21",
    "@uiw/codemirror-themes": "^4.25.4",
    "@uiw/react-codemirror": "^4.25.4",
    "axios": "^1.13.5",
    "class-variance-authority": "^0.7.1",
