    "jsdom": "^28.1.0",
    "license-checker": "^25.0.1",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "tailwindcss": "^4.1.18",
    "ts-prune": "^0.10.3",
