{
  "uid": "sublarr-database",
  "title": "Sublarr Database",
  "description": "Database performance: connection pool, query timing, cache metrics, Redis status",
  "tags": ["sublarr", "database"],
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "30s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "fiscalYearStartMonth": 0,
  "liveNow": false,
  "style": "dark",
  "editable": true,
  "graphTooltip": 1,
  "panels": [
    {
      "type": "row",
      "title": "Connection Pool",
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "id": 1
    },
    {
      "type": "stat",
      "title": "Pool Status",
      "description": "Connection pool size, checked out connections, and overflow count",
      "gridPos": { "h": 6, "w": 12, "x": 0, "y": 1 },
      "id": 2,
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 8 },
              { "color": "red", "value": 15 }
            ]
          }
        }
      },
      "targets": [
        {
          "expr": "sublarr_db_pool_size",
          "legendFormat": "Pool Size",
          "refId": "A"
        },
        {
          "expr": "sublarr_db_pool_checked_out",
          "legendFormat": "Checked Out",
          "refId": "B"
        },
        {
          "expr": "sublarr_db_pool_overflow",
          "legendFormat": "Overflow",
          "refId": "C"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "textMode": "auto",
        "colorMode": "value",
        "graphMode": "area",
        "orientation": "horizontal"
      }
    },
    {
      "type": "gauge",
      "title": "Pool Utilization",
      "description": "Percentage of pool connections currently checked out",
      "gridPos": { "h": 6, "w": 12, "x": 12, "y": 1 },
      "id": 3,
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 70 },
              { "color": "red", "value": 90 }
            ]
          }
        }
      },
      "targets": [
        {
          "expr": "(sublarr_db_pool_checked_out / sublarr_db_pool_size) * 100",
          "legendFormat": "Utilization",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "showThresholdLabels": false,
        "showThresholdMarkers": true
      }
    },
    {
      "type": "row",
      "title": "Query Performance",
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 7 },
      "id": 4
    },
    {
      "type": "timeseries",
      "title": "Query Duration (p95)",
      "description": "95th percentile database query duration by operation type",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
      "id": 5,
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10,
            "showPoints": "auto",
            "spanNulls": false
          },
          "unit": "s",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 0.1 },
              { "color": "red", "value": 0.5 }
            ]
          }
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(sublarr_db_query_duration_seconds_bucket[5m])) by (le, operation))",
          "legendFormat": "{{operation}}",
          "refId": "A"
        }
      ],
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max"] },
        "tooltip": { "mode": "multi" }
      }
    },
    {
      "type": "timeseries",
      "title": "Query Rate",
      "description": "Database query rate by operation type (select, insert, update, delete)",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
      "id": 6,
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 15,
            "showPoints": "auto",
            "spanNulls": false
          },
          "unit": "ops"
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "rate(sublarr_db_queries_total[5m])",
          "legendFormat": "{{operation}}",
          "refId": "A"
        }
      ],
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["sum", "mean"] },
        "tooltip": { "mode": "multi" }
      }
    },
    {
      "type": "row",
      "title": "Cache",
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 16 },
      "id": 7
    },
    {
      "type": "gauge",
      "title": "Cache Hit Rate",
      "description": "Percentage of cache requests that are hits (higher is better)",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 17 },
      "id": 8,
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 50 },
              { "color": "green", "value": 80 }
            ]
          }
        }
      },
      "targets": [
        {
          "expr": "(rate(sublarr_cache_hits_total[5m]) / (rate(sublarr_cache_hits_total[5m]) + rate(sublarr_cache_misses_total[5m]))) * 100",
          "legendFormat": "{{backend}}",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "showThresholdLabels": false,
        "showThresholdMarkers": true
      }
    },
    {
      "type": "stat",
      "title": "Cache Size",
      "description": "Number of items currently in cache",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 17 },
      "id": 9,
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 10000 },
              { "color": "red", "value": 100000 }
            ]
          }
        }
      },
      "targets": [
        {
          "expr": "sublarr_cache_size",
          "legendFormat": "{{backend}}",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "colorMode": "value",
        "graphMode": "area"
      }
    },
    {
      "type": "row",
      "title": "Redis & Queue",
      "collapsed": true,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 25 },
      "id": 10,
      "panels": [
        {
          "type": "stat",
          "title": "Redis Status",
          "description": "Redis connection status (1=connected, 0=disconnected)",
          "gridPos": { "h": 6, "w": 6, "x": 0, "y": 26 },
          "id": 11,
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
          "fieldConfig": {
            "defaults": {
              "color": { "mode": "thresholds" },
              "thresholds": {
                "mode": "absolute",
                "steps": [
                  { "color": "red", "value": null },
                  { "color": "green", "value": 1 }
                ]
              },
              "mappings": [
                { "options": { "0": { "text": "Disconnected", "color": "red" } }, "type": "value" },
                { "options": { "1": { "text": "Connected", "color": "green" } }, "type": "value" }
              ]
            }
          },
          "targets": [
            {
              "expr": "sublarr_redis_connected",
              "legendFormat": "Redis",
              "refId": "A"
            }
          ],
          "options": {
            "reduceOptions": { "calcs": ["lastNotNull"] },
            "colorMode": "background",
            "graphMode": "none"
          }
        },
        {
          "type": "stat",
          "title": "Redis Memory",
          "description": "Redis server memory usage",
          "gridPos": { "h": 6, "w": 6, "x": 6, "y": 26 },
          "id": 12,
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
          "fieldConfig": {
            "defaults": {
              "color": { "mode": "thresholds" },
              "unit": "bytes",
              "thresholds": {
                "mode": "absolute",
                "steps": [
                  { "color": "green", "value": null },
                  { "color": "yellow", "value": 104857600 },
                  { "color": "red", "value": 536870912 }
                ]
              }
            }
          },
          "targets": [
            {
              "expr": "sublarr_redis_memory_used_bytes",
              "legendFormat": "Memory",
              "refId": "A"
            }
          ],
          "options": {
            "reduceOptions": { "calcs": ["lastNotNull"] },
            "colorMode": "value",
            "graphMode": "area"
          }
        },
        {
          "type": "timeseries",
          "title": "Queue Depth",
          "description": "Job queue size, active jobs, and failed jobs over time",
          "gridPos": { "h": 6, "w": 12, "x": 12, "y": 26 },
          "id": 13,
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
          "fieldConfig": {
            "defaults": {
              "color": { "mode": "palette-classic" },
              "custom": {
                "lineWidth": 2,
                "fillOpacity": 15,
                "showPoints": "auto",
                "spanNulls": false
              }
            },
            "overrides": [
              {
                "matcher": { "id": "byName", "options": "Failed" },
                "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }]
              }
            ]
          },
          "targets": [
            {
              "expr": "sublarr_queue_size",
              "legendFormat": "Queued",
              "refId": "A"
            },
            {
              "expr": "sublarr_queue_active_jobs",
              "legendFormat": "Active",
              "refId": "B"
            },
            {
              "expr": "sublarr_queue_failed_jobs",
              "legendFormat": "Failed",
              "refId": "C"
            }
          ],
          "options": {
            "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max"] },
            "tooltip": { "mode": "multi" }
          }
        }
      ]
    }
  ],
  "templating": {
    "list": [
      {
        "name": "DS_PROMETHEUS",
        "type": "datasource",
        "query": "prometheus",
        "current": { "text": "Prometheus", "value": "Prometheus" },
        "hide": 0
      }
    ]
  },
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": { "type": "grafana", "uid": "-- Grafana --" },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  }
}
