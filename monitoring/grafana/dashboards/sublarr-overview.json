{
  "uid": "sublarr-overview",
  "title": "Sublarr Overview",
  "description": "Main Sublarr dashboard: translations, providers, HTTP API, system resources",
  "tags": ["sublarr"],
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "30s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "fiscalYearStartMonth": 0,
  "liveNow": false,
  "style": "dark",
  "editable": true,
  "graphTooltip": 1,
  "panels": [
    {
      "type": "row",
      "title": "System Overview",
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "id": 1
    },
    {
      "type": "timeseries",
      "title": "Translation Operations",
      "description": "Translation rate by status (success, failed, skipped)",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 1 },
      "id": 2,
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 20,
            "pointSize": 5,
            "showPoints": "auto",
            "spanNulls": false
          },
          "unit": "ops"
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "failed" },
            "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byName", "options": "success" },
            "properties": [{ "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }]
          }
        ]
      },
      "targets": [
        {
          "expr": "rate(sublarr_translation_total[5m])",
          "legendFormat": "{{status}} ({{format}})",
          "refId": "A"
        }
      ],
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["sum", "mean"] },
        "tooltip": { "mode": "multi" }
      }
    },
    {
      "type": "stat",
      "title": "Active Jobs",
      "description": "Current job queue depth and actively running jobs",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 1 },
      "id": 3,
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 10 },
              { "color": "red", "value": 50 }
            ]
          }
        }
      },
      "targets": [
        {
          "expr": "sublarr_job_queue_size",
          "legendFormat": "Queued Jobs",
          "refId": "A"
        },
        {
          "expr": "sublarr_queue_active_jobs",
          "legendFormat": "Active Jobs",
          "refId": "B"
        },
        {
          "expr": "sublarr_queue_size",
          "legendFormat": "Queue Depth",
          "refId": "C"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "textMode": "auto",
        "colorMode": "value",
        "graphMode": "area"
      }
    },
    {
      "type": "row",
      "title": "Provider Performance",
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 9 },
      "id": 4
    },
    {
      "type": "timeseries",
      "title": "Provider Search Rate",
      "description": "Provider search operations per second by provider and status",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 10 },
      "id": 5,
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10,
            "showPoints": "auto",
            "spanNulls": false
          },
          "unit": "ops"
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "rate(sublarr_provider_search_total[5m])",
          "legendFormat": "{{provider}} ({{status}})",
          "refId": "A"
        }
      ],
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["sum"] },
        "tooltip": { "mode": "multi" }
      }
    },
    {
      "type": "timeseries",
      "title": "Provider Download Rate",
      "description": "Provider download operations per second by provider",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 10 },
      "id": 6,
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10,
            "showPoints": "auto",
            "spanNulls": false
          },
          "unit": "ops"
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "rate(sublarr_provider_download_total[5m])",
          "legendFormat": "{{provider}} ({{format}})",
          "refId": "A"
        }
      ],
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["sum"] },
        "tooltip": { "mode": "multi" }
      }
    },
    {
      "type": "row",
      "title": "HTTP API",
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 18 },
      "id": 7
    },
    {
      "type": "timeseries",
      "title": "Request Duration (p95)",
      "description": "95th percentile HTTP request duration by endpoint",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 19 },
      "id": 8,
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10,
            "showPoints": "auto",
            "spanNulls": false
          },
          "unit": "s",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 1 },
              { "color": "red", "value": 5 }
            ]
          }
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(sublarr_http_request_duration_seconds_bucket[5m])) by (le, endpoint))",
          "legendFormat": "{{endpoint}}",
          "refId": "A"
        }
      ],
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max"] },
        "tooltip": { "mode": "multi" }
      }
    },
    {
      "type": "timeseries",
      "title": "Request Rate",
      "description": "HTTP request rate by status code",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 19 },
      "id": 9,
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 15,
            "showPoints": "auto",
            "spanNulls": false
          },
          "unit": "reqps"
        },
        "overrides": [
          {
            "matcher": { "id": "byRegexp", "options": ".*5\\d{2}.*" },
            "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byRegexp", "options": ".*4\\d{2}.*" },
            "properties": [{ "id": "color", "value": { "fixedColor": "orange", "mode": "fixed" } }]
          }
        ]
      },
      "targets": [
        {
          "expr": "sum(rate(sublarr_http_requests_total[5m])) by (status)",
          "legendFormat": "HTTP {{status}}",
          "refId": "A"
        }
      ],
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["sum", "mean"] },
        "tooltip": { "mode": "multi" }
      }
    },
    {
      "type": "row",
      "title": "System Resources",
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 27 },
      "id": 10
    },
    {
      "type": "gauge",
      "title": "CPU Usage",
      "description": "Current CPU usage percentage",
      "gridPos": { "h": 6, "w": 6, "x": 0, "y": 28 },
      "id": 11,
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 60 },
              { "color": "red", "value": 85 }
            ]
          }
        }
      },
      "targets": [
        {
          "expr": "sublarr_cpu_usage_percent",
          "legendFormat": "CPU",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "showThresholdLabels": false,
        "showThresholdMarkers": true
      }
    },
    {
      "type": "stat",
      "title": "Memory Usage",
      "description": "Process memory usage (RSS)",
      "gridPos": { "h": 6, "w": 6, "x": 6, "y": 28 },
      "id": 12,
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "unit": "bytes",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 536870912 },
              { "color": "red", "value": 1073741824 }
            ]
          }
        }
      },
      "targets": [
        {
          "expr": "sublarr_memory_usage_bytes",
          "legendFormat": "Memory",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "colorMode": "value",
        "graphMode": "area"
      }
    },
    {
      "type": "stat",
      "title": "Database Size",
      "description": "SQLite database file size",
      "gridPos": { "h": 6, "w": 6, "x": 12, "y": 28 },
      "id": 13,
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "unit": "bytes",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 104857600 },
              { "color": "red", "value": 524288000 }
            ]
          }
        }
      },
      "targets": [
        {
          "expr": "sublarr_database_size_bytes",
          "legendFormat": "DB Size",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "colorMode": "value",
        "graphMode": "none"
      }
    },
    {
      "type": "table",
      "title": "Circuit Breakers",
      "description": "Provider circuit breaker states (0=closed, 1=open, 2=half_open)",
      "gridPos": { "h": 6, "w": 6, "x": 18, "y": 28 },
      "id": 14,
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": {
          "custom": { "align": "auto" }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Value" },
            "properties": [
              {
                "id": "mappings",
                "value": [
                  { "options": { "0": { "text": "CLOSED", "color": "green" } }, "type": "value" },
                  { "options": { "1": { "text": "OPEN", "color": "red" } }, "type": "value" },
                  { "options": { "2": { "text": "HALF_OPEN", "color": "yellow" } }, "type": "value" }
                ]
              }
            ]
          }
        ]
      },
      "targets": [
        {
          "expr": "sublarr_circuit_breaker_state",
          "legendFormat": "{{provider}}",
          "format": "table",
          "instant": true,
          "refId": "A"
        }
      ],
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": { "Time": true, "__name__": true, "instance": true, "job": true },
            "renameByName": { "provider": "Provider", "Value": "State" }
          }
        }
      ],
      "options": {
        "showHeader": true,
        "footer": { "show": false }
      }
    }
  ],
  "templating": {
    "list": [
      {
        "name": "DS_PROMETHEUS",
        "type": "datasource",
        "query": "prometheus",
        "current": { "text": "Prometheus", "value": "Prometheus" },
        "hide": 0
      }
    ]
  },
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": { "type": "grafana", "uid": "-- Grafana --" },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  }
}
